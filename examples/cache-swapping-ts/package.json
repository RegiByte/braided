{
  "name": "braided-example-cache-swapping-ts",
  "version": "1.0.0",
  "type": "module",
  "description": "TypeScript cache swapping example - same code, different implementations with full type safety",
  "private": true,
  "scripts": {
    "start": "tsx system.ts",
    "start:memory": "CACHE_TYPE=memory tsx system.ts",
    "start:redis": "CACHE_TYPE=redis tsx system.ts",
    "test": "tsx test-system.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "braided": "file:../..",
    "express": "^4.18.2",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}

