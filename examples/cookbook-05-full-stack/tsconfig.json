{
  "compilerOptions": {
    "target": "es2020",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "types": ["node"],
    "lib": ["es2022"],
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "noUncheckedIndexedAccess": false,
    "exactOptionalPropertyTypes": true,
    "strict": true,
    "jsx": "react-jsx",
    "verbatimModuleSyntax": false,
    "isolatedModules": true,
    "noUncheckedSideEffectImports": true,
    "moduleDetection": "force",
    "skipLibCheck": true,
    "pretty": true
  }
}

