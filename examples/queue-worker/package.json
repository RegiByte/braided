{
  "name": "queue-worker-example",
  "version": "1.0.0",
  "description": "Queue worker with BullMQ and Redis using Braided for lifecycle management",
  "main": "system.js",
  "type": "commonjs",
  "scripts": {
    "start": "node system.js",
    "dev": "node --watch system.js",
    "docker:up": "docker-compose up -d",
