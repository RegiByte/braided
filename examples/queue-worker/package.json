    "docker:logs": "docker-compose logs -f redis"
  },
  "keywords": [
    "braided",
    "queue",
    "bullmq",
    "redis",
    "worker"
  ],
  "author": "RegiByte",
  "license": "ISC",
  "dependencies": {
    "braided": "^0.0.4",
    "bullmq": "^5.28.2",
    "ioredis": "^5.4.2"
  }
}

